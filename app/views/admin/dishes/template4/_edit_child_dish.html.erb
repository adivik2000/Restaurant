<% form_for(@dish) do |f| %>
	<%= f.hidden_field :parent_id %><br />  
<div class="field">
	<%= f.label :name %><br />
	<%= f.text_field :name %><br />  
</div>
<div class="field">
	<%= f.label :description %><br />
	<%= f.text_field :description %><br />  
</div>
<div class="field">
	<%= f.label :price %><br />
	<%= f.text_field :price_parse %><br />
</div><br />
<div class="update_button">
	<%= f.submit "Save" %>
</div>
<% end %>
